<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:binding="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="checkModel"
            type="cangjie.scale.sorting.vm.ScaleViewModel" />

        <variable
            name="info"
            type="cangjie.scale.sorting.entity.OrderInfo" />


        <variable
            name="adapterChecked"
            type="com.chad.library.adapter.base.BaseQuickAdapter" />

        <variable
            name="adapterCheck"
            type="com.chad.library.adapter.base.BaseQuickAdapter" />

        <variable
            name="imgAdapter"
            type="com.chad.library.adapter.base.BaseQuickAdapter" />

        <import type="android.view.View" />
    </data>


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@drawable/shape_main_bg"
            android:padding="@dimen/dp10">

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp25"
                android:layout_marginTop="@dimen/dp5"
                android:layout_marginBottom="@dimen/dp25"
                android:src="@drawable/ic_main_logo"
                android:visibility="invisible" />

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:drawableStart="@drawable/ic_back"
                android:drawablePadding="@dimen/dp8"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/dp25"
                android:paddingEnd="@dimen/dp25"
                android:text="@string/str_check"
                android:textColor="@color/white"
                android:textSize="@dimen/sp20"
                binding:onClickCommand="@{checkModel.detailClose}" />


        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="3">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="14dp"
                android:layout_weight="2"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dp15"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/dp25"
                    android:paddingTop="@dimen/dp15"
                    android:paddingEnd="@dimen/dp15">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableStart="@drawable/ic_delivery_company"
                        android:drawablePadding="@dimen/dp5"
                        android:gravity="center_vertical"
                        android:text="@{@string/str_company+info.supplier_name}"
                        android:textColor="@color/black_3"
                        android:textSize="@dimen/sp18" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_4"
                        android:drawableStart="@drawable/ic_trade_no"
                        android:drawablePadding="@dimen/dp5"
                        android:gravity="center_vertical"
                        android:text="@{@string/str_strade_no+info.trade_no}"
                        android:textColor="@color/black_3"
                        android:textSize="@dimen/sp18" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_4"
                        android:drawableStart="@drawable/ic_delivery_date"
                        android:drawablePadding="@dimen/dp5"
                        android:gravity="center_vertical"
                        android:text="@{@string/str_delivery_date+info.collect_time}"
                        android:textColor="@color/black_3"
                        android:textSize="@dimen/sp18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/dp5"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/dp25"
                    android:paddingEnd="@dimen/dp25">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            binding:onClickCommand="@{checkModel.checkCommand}">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="待验收商品"
                                android:textColor="#DD5454"
                                android:textSize="@dimen/sp18" />

                            <View
                                android:layout_width="100dp"
                                android:layout_height="2dp"
                                android:layout_marginTop="@dimen/dp10"
                                android:background="#DD5454"
                                android:visibility="@{checkModel.showStatusFiled==0?View.VISIBLE:View.INVISIBLE}" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp30"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            binding:onClickCommand="@{checkModel.unCheckCommand}">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="已验收商品"
                                android:textColor="#51A644"
                                android:textSize="@dimen/sp18" />

                            <View
                                android:layout_width="100dp"
                                android:layout_height="2dp"
                                android:layout_marginTop="@dimen/dp10"
                                android:background="#51A644"
                                android:visibility="@{checkModel.showStatusFiled==1?View.VISIBLE:View.INVISIBLE}" />
                        </LinearLayout>
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp8"
                            android:visibility="@{checkModel.showStatusFiled==0?View.VISIBLE:View.GONE}"
                            binding:cardBackgroundColor="@color/white"
                            binding:cardCornerRadius="@dimen/dp5"
                            binding:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/shape_choose_date"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:background="@drawable/shape_table_top"
                                    android:orientation="horizontal">

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.3"
                                        android:gravity="center"
                                        android:text="序号"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="商品名称"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="规格"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="采购数量"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="单位"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="计量方式"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.4"
                                        android:gravity="center"
                                        android:text="计损"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />
                                </LinearLayout>

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/ry_order_check"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    binding:adapter="@{adapterCheck}"
                                    binding:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp8"
                            android:visibility="@{checkModel.showStatusFiled==1?View.VISIBLE:View.GONE}"
                            binding:cardBackgroundColor="@color/white"
                            binding:cardCornerRadius="@dimen/dp5"
                            binding:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/shape_choose_date"
                                android:orientation="vertical">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:background="@drawable/shape_table_top"
                                    android:orientation="horizontal">

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.4"
                                        android:gravity="center"
                                        android:text="序号"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="商品名称"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="规格"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="采购数量"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="验收数量"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />

                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:text="验收单位"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />


                                    <View
                                        android:layout_width="1dp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray_c" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.4"
                                        android:gravity="center"
                                        android:text="操作"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp16" />
                                </LinearLayout>

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/ry_order_checked"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    binding:adapter="@{adapterChecked}"
                                    binding:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </RelativeLayout>


                </LinearLayout>


            </LinearLayout>

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dp15"
                android:layout_marginEnd="@dimen/dp25"
                android:layout_marginBottom="@dimen/dp15"
                android:layout_weight="1"
                binding:cardBackgroundColor="@color/white"
                binding:cardCornerRadius="@dimen/dp8"
                binding:cardElevation="@dimen/dp_4">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <androidx.camera.view.PreviewView
                            android:id="@+id/previewView"
                            android:layout_width="match_parent"
                            android:layout_height="180dp" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/ry_img"
                            android:layout_width="match_parent"
                            android:layout_height="80dp"
                            android:layout_margin="@dimen/dp10"
                            binding:adapter="@{imgAdapter}" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_toStartOf="@+id/btn_reset_zero">

                                <com.cangjie.frame.kit.DigitalText
                                    android:id="@+id/tv_current_weight"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_toStartOf="@+id/tv_sacal_unit"
                                    android:background="@color/black"
                                    android:gravity="center"
                                    android:padding="12dp"
                                    android:text="0.00"
                                    android:textColor="@android:color/holo_red_dark"
                                    android:textSize="50sp" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/tv_sacal_unit"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_centerVertical="true"
                                    android:padding="@dimen/dp10"
                                    android:text="斤"
                                    android:textColor="@color/black_3"
                                    android:textSize="@dimen/sp24" />
                            </RelativeLayout>

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btn_reset_zero"
                                android:layout_width="65dp"
                                android:layout_height="55dp"
                                android:layout_alignParentEnd="true"
                                android:layout_centerVertical="true"
                                android:background="#6AC75B"
                                android:gravity="center"
                                android:text="置零"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp16"
                                android:visibility="gone"
                                binding:onClickCommand="@{checkModel.resetZeroCmd}" />

                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp10"
                            android:orientation="vertical"
                            android:paddingStart="@dimen/dp10"
                            android:paddingEnd="@dimen/dp10">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/tv_delivery_name"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dp5"
                                android:textColor="@color/black_3"
                                android:textSize="@dimen/sp18" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/tv_delivery_type"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dp5"
                                android:textColor="@color/black_3"
                                android:textSize="@dimen/sp18" />


                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/tv_receive_count"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dp5"
                                android:textColor="@color/black_3"
                                android:textSize="@dimen/sp18" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/tv_delivery_count"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dp5"
                                android:textColor="@color/black_3"
                                android:textSize="@dimen/sp18" />

                        </LinearLayout>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:paddingStart="@dimen/dp10"
                            android:paddingEnd="@dimen/dp15">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="收货价格："
                                android:textColor="@color/black_3"
                                android:textSize="@dimen/sp18" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/tv_receive_price"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0.00"
                                    android:textColor="@color/black_3"
                                    android:textSize="@dimen/sp18"
                                    android:visibility="gone" />

                                <LinearLayout
                                    android:id="@+id/ll_edit_price"
                                    android:layout_width="100dp"
                                    android:layout_height="40dp"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal">

                                    <androidx.appcompat.widget.AppCompatEditText
                                        android:id="@+id/edit_current_price"
                                        android:layout_width="100dp"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/shape_input_unit"
                                        android:gravity="center"
                                        android:hint="请输入"
                                        android:inputType="numberDecimal"
                                        android:textColor="@color/black_3"
                                        android:textColorHint="#cccccc"
                                        android:textSize="@dimen/sp18" />

                                </LinearLayout>

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="@dimen/dp5"
                                    android:text="元"
                                    android:textColor="@color/black_3"
                                    android:textSize="@dimen/sp18" />
                            </LinearLayout>
                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:layout_centerVertical="true"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:paddingStart="@dimen/dp10"
                                android:paddingEnd="@dimen/dp15">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="本次数量："
                                    android:textColor="@color/black_3"
                                    android:textSize="@dimen/sp18" />

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal">

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:id="@+id/tv_delivery_current"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp18" />

                                    <LinearLayout
                                        android:id="@+id/ll_edit_count"
                                        android:layout_width="wrap_content"
                                        android:layout_height="40dp"
                                        android:gravity="center_vertical"
                                        android:orientation="horizontal">

                                        <androidx.appcompat.widget.AppCompatEditText
                                            android:id="@+id/edit_current_count"
                                            android:layout_width="100dp"
                                            android:layout_height="match_parent"
                                            android:background="@drawable/shape_input_unit"
                                            android:gravity="center"
                                            android:hint="请输入"
                                            android:inputType="numberDecimal"
                                            android:textColor="@color/black_3"
                                            android:textColorHint="#cccccc"
                                            android:textSize="@dimen/sp18" />

                                    </LinearLayout>

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:id="@+id/tv_input_unit"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:padding="@dimen/dp5"
                                        android:textColor="@color/black_3"
                                        android:textSize="@dimen/sp18" />
                                </LinearLayout>
                            </LinearLayout>

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btn_remove"
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:layout_alignParentEnd="true"
                                android:layout_centerVertical="true"
                                android:layout_marginEnd="20dp"
                                android:background="@drawable/shape_clear_btn"
                                android:paddingStart="10dp"
                                android:paddingEnd="10dp"
                                android:text="手动去皮"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp16"
                                binding:onClickCommand="@{checkModel.removeShellCommand}" />
                        </RelativeLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp10"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:padding="@dimen/dp15">

                            <androidx.appcompat.widget.AppCompatButton
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:layout_marginEnd="@dimen/dp8"
                                android:layout_weight="1"
                                android:background="@drawable/shape_clear_btn"
                                android:text="重新验收"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp16"
                                binding:onClickCommand="@{checkModel.clearCommand}" />

                            <androidx.appcompat.widget.AppCompatButton
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:layout_marginEnd="@dimen/dp8"
                                android:layout_weight="1"
                                android:background="@drawable/shape_take_photo"
                                android:text="记录拍照"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp16"
                                binding:onClickCommand="@{checkModel.takePhoto}" />

                            <androidx.appcompat.widget.AppCompatButton
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:layout_weight="1"
                                android:background="@drawable/shape_login_btn"
                                android:text="完成提交"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp16"
                                binding:onClickCommand="@{checkModel.submitCommand}" />
                        </LinearLayout>
                    </LinearLayout>


                </RelativeLayout>

            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
</layout>
